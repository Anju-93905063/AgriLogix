{
    "version": 2,
    "functions": {
        "api/index.ts": {
            "includeFiles": "dist/public/**"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/dist/public/assets/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.ts"
        }
    ]
}